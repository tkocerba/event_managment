<%= title @event.title  %>
<% at_page %>

<div class="row">
    <div class="col-2 offset-10">
      <%= link_to 'Back to category', category_path(@category), class: 'btn btn-primary btn-sm mb-5 float-right'%>
    </div>
  </div>
  
<p class="lead">
    <%= @event.description %> <br><br>
    <strong>Starts at:</strong> <%= @event.starts_at %> <br>
    <strong>Ends at:</strong> <%= @event.ends_at %>
</p>
