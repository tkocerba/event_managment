<div class="card mb-5">
  <div class="card-block">
    <h4 class="card-title mt-2 mb-2 ml-2"><%= link_to event.title, category_event_path(event.category, event) %></h4>
    <h6 class="card-subtitle mb-2 ml-2 text-muted">From <%= event.starts_at %> to <%= event.ends_at %></h6>
    <p class="card-text ml-2 mb-2"><%= event.description %></p>
  </div>
</div>